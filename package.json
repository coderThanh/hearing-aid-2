{
  "name": "pm-theme-child",
  "version": "1.0.0",
  "description": "Child theme for PT theme with Tailwind CSS",
  "main": "index.js",
  "scripts": {
    "build": "tailwindcss -i ./8pm-assest/css/tailwind.css -o ./8pm-assest/css/tailwind-output.css --minify",
    "watch": "tailwindcss -i ./8pm-assest/css/tailwind.css -o ./8pm-assest/css/tailwind-output.css --watch",
    "dev": "concurrently \"npx gulp\" \"npm run watch\"",
    "start": "concurrently \"npx gulp\" \"npm run watch\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.1.0",
    "gulp": "^5.0.1",
    "gulp-file-include": "^2.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-nunjucks-render": "^2.2.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17"
  }
}
